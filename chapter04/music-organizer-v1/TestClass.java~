public class TestClass
{
    private MusicOrganizer musicOrg1;

    public TestClass()
    {
        
    }

    public void setUpData()
    {
        musicOrg1 = new MusicOrganizer();
        musicOrg1.addFile("Davynals - I touch myself.mp3");
        musicOrg1.addFile("Madonnnnnnnna - Like a fukn virgin.flac");
        musicOrg1.addFile("David Guerta - Fuk me im famouz.mp3");   
    }
    
    public void testOne()
    {
        musicOrg1.checkIndex(1); //should return da madonnas
        musicOrg1.checkIndex(2); //should error cause it is outside da limit
        
        musicOrg1.validIndex(2); //true
        musicOrg1.validIndex(3); //false
    }
    
    public void testTwo()
    {
        musicOrg1.listFileRewrite(1); //true
        musicOrg1.listFileRewrite(3); //false
    }
}